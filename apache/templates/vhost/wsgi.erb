<% if @wsgiscriptalias and ! @wsgiscriptalias.empty? -%>
<IfModule wsgi_module>
<% if @wsgidaemonprocess and ! @wsgidaemonprocess.empty? -%>
  WSGIDaemonProcess <%= @wsgidaemonprocess %>
<% end -%>
<% if @wsgiprocessgroup and ! @wsgiprocessgroup.empty? -%>
  WSGIProcessGroup <%= @wsgiprocessgroup %>
<% end -%>
  WSGIScriptAlias <%= @wsgiscriptalias %>
</IfModule>
<% end -%>